{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "esModuleInterop": false,
    "target": "es5",
    "allowJs": false,
    "module": "esnext",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "newLine": "LF",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "noEmit": false,
    "noEmitHelpers": true,
    "importHelpers": true,
    "strictNullChecks": true,
    "declaration": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "removeComments": false,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "types": ["node", "styled-components"],
    "lib": ["esnext", "dom"],
    "baseUrl": "src",
    "paths": {
    }
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
